{
  "name": "owserver (1-Wire)",
  "version": "0.1.0",
  "slug": "owserver",
  "description": "onewire server to read 1-Wire devices",
  "url": "https://github.com/lrybak/hassio-addons/owserver",
  "startup": "system",
  "init": false,
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",  
  "ports": {
		"2121/tcp": 2121
  },
  "auto_uart": true,
  "options": {
		"owhttpd": false,
		"device": ""
  },
  "schema": {
		"owhttpd": "bool",
		"device": "str?"
  }
}