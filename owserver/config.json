{
  "name": "owserver (1-Wire)",
  "version": "0.2.0",
  "slug": "owserver",
  "description": "onewire server to read 1-Wire devices",
  "url": "https://github.com/lrybak/hassio-addons/owserver",
  "startup": "system",
  "init": false,
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",  
  "auto_uart": true,
  "options": {
		"owhttpd": true,
		"device": ""
  },
  "schema": {
		"owhttpd": "bool",
		"device": "str?"
  },

  "ingress": true,
  "panel_icon": "mdi:alpha-w-box"
}